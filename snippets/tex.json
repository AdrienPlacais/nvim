{
   "Frame": {
      "prefix": "fra",
      "body": [
         "\\begin{frame}{$1}",
         "   $2",
         "\\end{frame}"
      ],
      "description": "frame"
   },
   "Section": {
      "prefix": "sec",
      "body": ["\\section{$1}", "   $2"],
      "description": "section"
   },
   "Sub-Section": {
      "prefix": "ssec",
      "body": ["\\subsection{$1}", "   $2"],
      "description": "subsection"
   },
   "Itemize": {
      "prefix": "it",
      "body": ["\\begin{itemize}", "   \\item $1", "\\end{itemize}"],
      "description": "itemize"
   },
   "Enumerate": {
      "prefix": "enu",
      "body": ["\\begin{enumerate}", "   \\item $1", "\\end{enumerate}"],
      "description": "enumerate"
   },
   "Inline math environment": {
      "prefix": "$$",
      "body": ["\\( $1 \\) $2"],
      "description": "inline math"
   },
   "Emphasis env": {
      "prefix": "em",
      "body": ["\\emph{$1}$2"],
      "description": "emphasis"
   },
   "Alerted env": {
      "prefix": "al",
      "body": ["\\alert{$1}$2"],
      "description": "alerted text"
   },
   "Todo env": {
      "prefix": "to",
      "body": ["\\todo{$1}$2"],
      "description": "todo environment"
   }
}
