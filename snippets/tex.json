{
   "Frame": {
      "prefix": "fra",
      "body": [
         "\\begin{frame}{$1}",
         "   $2",
         "\\end{frame}"
      ],
      "description": "frame"
   },
   "Section": {
      "prefix": "sec",
      "body": ["\\section{$1}", "   $2"],
      "description": "section"
   },
   "Sub-Section": {
      "prefix": "ssec",
      "body": ["\\subsection{$1}", "   $2"],
      "description": "subsection"
   },
   "Itemize": {
      "prefix": "it",
      "body": ["\\begin{itemize}", "   \\item $1", "\\end{itemize}"],
      "description": "itemize"
   },
   "Enumerate": {
      "prefix": "enu",
      "body": ["\\begin{enumerate}", "   \\item $1", "\\end{enumerate}"],
      "description": "enumerate"
   },
   "Inline math environment": {
      "prefix": "$$",
      "body": ["\\( $1 \\) $2"],
      "description": "inline math"
   },
   "Emphasis env": {
      "prefix": "em",
      "body": ["\\emph{$1}$2"],
      "description": "emphasis"
   },
   "Alerted env": {
      "prefix": "al",
      "body": ["\\alert{$1}$2"],
      "description": "alerted text"
   },
   "Todo env": {
      "prefix": "to",
      "body": ["\\todo{$1}$2"],
      "description": "todo environment"
   },
   "Figure": {
      "prefix": "fig",
      "body": [
         "\\begin{figure}[hbtp]",
         "\\centering",
         "\\includegraphics[${1:width=\\textwidth}]{${2:example-image-a}}",
         "\\caption{$3}",
         "\\end{figure}",
         "\\vspace{-\\baselineskip}",
         "$4"
      ],
      "description": "fig env"
   },
   "Subfigure": {
      "prefix": "subfig",
      "body": [
         "\\begin{figure}[hbtp]",
         "\\begin{subfigure}[t]{.49\\textwidth}",
         "\\includegraphics[width=\\textwidth]{$1}",
         "\\caption{$2}",
         "\\end{subfigure}",
         "\\hfill",
         "\\begin{subfigure}[t]{.49\\textwidth}",
         "\\includegraphics[width=\\textwidth]{$3}",
         "\\caption{$4}",
         "\\end{subfigure}",
         "\\caption{%",
         "$5",
         "}",
         "\\end{figure}",
         "\\vspace{-\\baselineskip}",
         "$6"
      ],
      "description": "subfig env"
   },
   "si": {
     "prefix": "si",
     "body": ["\\si{$1}$2"],
     "description": "si command"
   },
   "SI": {
     "prefix": "SI",
     "body": ["\\SI{$1}{$2}$3"],
     "description": "SI command"
   },
   "texttt": {
     "prefix": "ttt",
     "body": ["\\texttt{$1}$2"],
     "description": "monofont env"
   },
   "columns": {
      "prefix": "col",
      "body": [
         "\\begin{columns}[${1:t}]",
         "\\begin{column}{${2:.50\\textwidth}}",
         "$3",
         "\\end{column}",
         "\\begin{column}{${4:.50\\textwidth}}",
         "$5",
         "\\end{column}",
         "\\end{columns}",
         "$6"
      ],
      "description": "columns env"
   }
}
