{
   // Pydocstring
   "pydocstring.formatter": "numpy",
   "pydocstring.enableFileAction": true,
   // Pyright
   "python.linting.flake8Enabled": true,
   "python.linting.pydocstyleEnabled": true,
   "python.pythonPath": "~/.config/nvim/misc/python_alias_for_coc-pyright.py",
   "pyright.organizeimports.provider": "isort",
   "pyright.inlayHint.functionReturnTypes": false,
   "pyright.inlayHint.variableTypes": false,
   "pyright.inlayHint.enable": false,
   "inlayHint.enable": false,
   "inlayHint.refreshOnInsertMode": false,
   // Vimtex
   "coc.source.vimtex.filetypes": ['tex', 'plaintex', 'latex', 'tikz'],
   // CoC
   "coc.preferences.jumpCommand": "vsplit"
}
